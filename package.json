{
  "name": "notes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server1": "cross-env DEBUG=notes:* PORT=3001 node ./app.mjs",
    "server2": "cross-env DEBUG=notes:* PORT=3002 node ./app.mjs",
