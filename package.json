    "debug": "~2.6.9",
    "express": "^4.18.2",
    "express-generator": "^4.2.0",
    "feather-icons": "4.25.x",
    "fs-extra": "^11.1.0",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "jquery": "3.5.x",
    "level": "^6.0.1",
